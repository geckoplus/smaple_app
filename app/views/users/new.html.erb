<% provide(:title, 'Sign up')%>
<h1>Sign up</h1>

<div class="row hero-unit">
	<div class="container">

		<div class="span6 offset3">
			<%= form_for(@user) do |f| %>
			<%= render 'fields', f: f %>
			<p>
				<%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
			</p>
			
		</div>
	</div>
<% end %>
</div>
